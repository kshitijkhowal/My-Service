  "event": [
    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Auto-set API key for protected endpoints",
          "if (pm.request.method !== 'GET' && !pm.request.url.toString().includes('/health')) {",
          "    pm.request.headers.add({",
          "        key: 'x-api-key',",
          "        value: pm.collectionVariables.get('API_KEY')",
          "    });",
          "}"
        ]
      }
    }
  ]
