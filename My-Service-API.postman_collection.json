            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"John Smith\",\n  \"bio\": \"Updated bio information\",\n  \"phone\": \"+1234567891\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/personal/profile/{{profileId}}",
